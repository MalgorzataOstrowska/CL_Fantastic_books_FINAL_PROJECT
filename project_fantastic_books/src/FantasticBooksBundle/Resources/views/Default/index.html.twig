                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="journalist" id="journalist" value=" journalist">
                        {{ 'occupation.journalist'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="broker" id="broker" value=" broker">
                        {{ 'occupation.broker'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="architect" id="architect" value=" architect">
                        {{ 'occupation.architect'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="bard" id="bard" value=" bard">
