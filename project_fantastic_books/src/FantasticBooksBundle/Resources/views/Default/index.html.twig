                        {{ 'occupation.musician'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="actor" id="actor" value=" actor">
                        {{ 'occupation.actor'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="carMechanic" id="carMechanic" value=" carMechanic">
