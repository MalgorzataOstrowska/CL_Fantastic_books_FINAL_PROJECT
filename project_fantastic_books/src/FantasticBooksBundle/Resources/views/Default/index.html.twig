{% extends "::base.html.twig" %}
{% block body %}
    <h2>{{ 'Character'| trans }}:</h2>

    <form action="{{ path('concat_data') }}" method="get">
        <div class="form-group">
            <h3>{{ 'name.Name'| trans }}:</h3>
            <div class="row">
                <div class="col-sm-2">
                    <label><input type="text" name="name" id="name"></label>
                </div>
            </div>
        </div>

        <div class="form-group">
            <h3>{{ 'gender.Gender'| trans }}:</h3>
            <div class="row">
                <div class="col-sm-2">
                    <label>
                        <input type="radio" name="gender" id="female" value="female">
                        {{ 'gender.female'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="radio" name="gender" id="male" value="male">
                        {{ 'gender.male'| trans }}
                    </label>
                </div>

                <div class="col-sm-2">
                    <label>
                        <input type="radio" name="gender" id="other" value="other">
                        {{ 'gender.other'| trans }}
                    </label>
                </div>
            </div>
        </div>

        <div class="form-group">
            <h3>{{ 'species.Species'| trans }}:</h3>
            <div class="row">
                <div class="col-sm-2">
                    <label>
                        <input type="radio" name="species" id="human" value="human">
                        {{ 'species.human'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="radio" name="species" id="shapeshifter" value="shapeshifter">
                        {{ 'species.shapeshifter'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="radio" name="species" id="notHuman" value="notHuman">
                        {{ 'species.not human'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="radio" name="species" id="childOfHumanAndNotHuman"
                               value="childOfHumanAndNotHuman">
                        {{ 'species.child of human and not human'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="radio" name="species" id="childOfDifferentNotHumans"
                               value="childOfDifferentNotHumans">
                        {{ 'species.child of different not humans'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="radio" name="species" id="other" value="other">
                        {{ 'species.other'| trans }}
                    </label>
                </div>
            </div>
        </div>

        <div class="form-group">
            <h3>{{ 'age.Age'| trans }}:</h3>
            <div class="row">
                <div class="col-sm-2">
                    <label>
                        <input type="radio" name="age" id="child" value="child">
                        {{ 'age.child'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="radio" name="age" id="teenager" value="teenager">
                        {{ 'age.teenager'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                        <label>
                            <input type="radio" name="age" id="adult" value="adult">
                            {{ 'age.adult'| trans }}
                        </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="radio" name="age" id="ancient" value="ancient">
                        {{ 'age.ancient'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="radio" name="age" id="otherAge" value="otherAge">
                        {{ 'age.other'| trans }}
                    </label>
                </div>
            </div>
        </div>

        <div class="form-group">
            <h3>{{ 'ability.Abilities'| trans }}:</h3>
            <div class="row">
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="ability[]" id="magic" value="magic">
                        {{ 'ability.magic'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="ability[]" id="flying" value="flying">
                        {{ 'ability.flying'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="ability[]" id="teleportation"
