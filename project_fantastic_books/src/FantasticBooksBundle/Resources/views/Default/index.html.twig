                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="cook" id="cook" value=" cook">
                        {{ 'occupation.cook'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="otherOccupation" id="otherOccupation"
                               value="otherOccupation">
                        {{ 'occupation.other'| trans }}
                    </label>
                </div>
            </div>
        </div>

        <div class="form-group">
            <h3>{{ 'notHuman.Not human'| trans }}:</h3>
            <div class="row">
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="mythologicalCreature" id="mythologicalCreature"
