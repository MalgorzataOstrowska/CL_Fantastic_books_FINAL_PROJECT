                        {{ 'ability.telepathy'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="ability[]" id="foreseeFuture"
                               value="foreseeFuture">
                        {{ 'ability.foresee future'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="ability[]" id="seeingPast"
                               value="seeingPast">
                        {{ 'ability.seeing past'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="ability[]"
                               id="otherAbility" value="otherAbility">
                        {{ 'ability.other'| trans }}
                    </label>
                </div>
            </div>
        </div>

        <div class="form-group">
