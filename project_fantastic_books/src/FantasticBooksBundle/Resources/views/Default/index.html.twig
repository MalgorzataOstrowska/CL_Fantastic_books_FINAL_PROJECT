                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="otherNotHuman" id="otherNotHuman"
                               value="otherNotHuman">
                        {{ 'notHuman.other'| trans }}
                    </label>
                </div>
            </div>
        </div>

        <div class="form-group">
            <h3>{{ 'mythology.Mythology'| trans }}:</h3>
            <div class="row">
                <div class="col-sm-2">
                    <label>
