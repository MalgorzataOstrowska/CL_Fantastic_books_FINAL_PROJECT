                        {{ 'ability.teleportation'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="ability[]" id="telekinesis"
                               value="telekinesis">
                        {{ 'ability.telekinesis'| trans }}
                    </label>
                </div>
                <div class="col-sm-2">
                    <label>
                        <input type="checkbox" name="ability[]" id="invisibility"
